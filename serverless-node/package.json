{
  "name": "boilerplate_lambda",
  "version": "1.0.0",
  "description": "Boilerplate Serverless Application",
  "private": true,
  "authors": [
    "Samuel Cardoso"
  ],
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "./node_modules/.bin/serverless offline start --stage local",
    "debug": "npm run-script build && NODE_ENV=dev DEBUG=* node --inspect ./node_modules/.bin/serverless offline --noTimeout",
    "clean": "rimraf .build",
    "lint": "eslint ./app/**/*.ts ./test/**/*.ts",
    "format": "tsfmt -r",
    "prepublish": "npm run build",
    "pretest": "cross-env npm build && npm run lint",
    "test": "cross-env mocha",
    "tsc": "tsc"
  },
  "dependencies": {
    "aws-lambda": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "latest",
    "chai-as-promised": "^7.1.1",
    "chai-http": "latest",
    "cross-env": "latest",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "latest",
    "nsrun": "^1.4.1",
    "nyc": "latest",
    "rimraf": "latest",
    "serverless": "^1.43.0",
    "serverless-offline": "^12.0.3",
    "serverless-plugin-typescript": "^1.1.5",
    "serverless-prune-plugin": "^1.3.2",
    "sinon": "^7.2.4",
    "typescript": "^3.5.3"
  }
}
